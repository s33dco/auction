<header>
	<h1>Edit <%=@category.title%></h1>
</header>
<article>
	<%=form_with(model:@category, local:true) do |f| %>
		<%= render 'category_fields', f:f %>
		<%= f.submit %>
	<% end %>
</article>

<% content_for(:side_links) do %>
	<li class='contextual'>
		<%=link_to 'new category', new_category_path %>
	</li>	
<% end %>
